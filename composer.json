	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "David Sveningsson",
			"email": "ext@nitroxy.com"
		},
		{
			"name": "Andreas Tarandi",
			"email": "torandi@nitroxy.com"
		}
